    "dotenv": "^16.4.7",
    "ethers": "^6.13.4",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^2.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.7.0",
