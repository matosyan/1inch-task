    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.2",
    "@types/bignumber.js": "^5.0.0",
    "@types/commander": "^2.12.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
